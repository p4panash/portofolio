{
	"buildCommand": "pnpm build",
	"outputDirectory": "build",
	"headers": [
		{
			"source": "/images/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/(.*\\.(css|js|woff|woff2|png|jpg|jpeg|webp|avif))",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/(.*\\.html)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600, must-revalidate"
				}
			]
		}
	]
}
